[
  {
    "name": "team", 
    "description": "the identifier of the team submitting the task",
    "re": "[^_]+"
  }, 
  {
    "name": "priority", 
    "description": "a integer (low = high priority) assigned by the participant",
    "re": "\\d+"
  }, 
  {
    "name": "segmentationType", 
    "description": "the way how the result segments were defined",
    "options": [
      {
        "code": "Ss", 
        "description": "speech sentence segmentation"
      }, 
      {
        "code": "Sp", 
        "description": "speech segment segmentation"
      }, 
      {
        "code": "Sh", 
        "description": "shot segmentation"
      }, 
      {
        "code": "F", 
        "description": "fixed length segmentation"
      }, 
      {
        "code": "L", 
        "description": "lexical cohesian segmentation"
      }, 
      {
        "code": "P", 
        "description": "use prosodic features for segmentation"
      }, 
      {
        "code": "O", 
        "description": "other segmentation"
      }
    ]
  }, 
  {
    "name": "visualFeatures", 
    "description": "the visual features being used",
    "options": [
      {
        "code": "A", 
        "description": "Provided AlexNet features"
      }, 
      {
        "code": "D", 
        "description": "Deep Learning features from known networks (e.g. from caffee model zoo https://github.com/BVLC/caffe/wiki/Model-Zoo)"
      }, 
      {
        "code": "O", 
        "description": "Other visual features"
      }, 
      {
        "code": "N", 
        "description": "No visual features"
      }
    ]
  }, 
  {
    "name": "asrFeatures", 
    "description": "the transcript being used",
    "options": [
      {
        "code": "I12", 
        "description": "LIMSI transcripts 2012"
      }, 
      {
        "code": "I16", 
        "description": "LIMSI transcripts 2016"
      }, 
      {
        "code": "U", 
        "description": "LIUM transcripts 2012"
      }, 
      {
        "code": "N", 
        "description": "No speech information"
      }
    ]
  }, 
  {
    "name": "additionalFeatures",
    "description": "other features being used, a concatenation of", 
    "options": [
      {
        "code": "M", 
        "description": "metadata provided with the video"
      }, 
      {
        "code": "O", 
        "description": "Other information"
      }, 
      {
        "code": "N", 
        "description": "No additional features"
      }
    ]
  }, 
  {
    "name": "runId", 
    "re": "[\\w-]{,20}",
    "description": "short identifier of your run (use - as space)"
  }
]
